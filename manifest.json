{
  "description": "Output Keen IO analytics data in the JavaScript console",
  "background": {"scripts": ["background.js"]},
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["log-keen-requests.js"],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "name": "Keen Debugger",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "webRequest",
    "tabs",
    "storage"
  ],
  "manifest_version": 2,
  "version": "1.3",
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
